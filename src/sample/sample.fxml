<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<AnchorPane fx:controller="sample.Controller"
            xmlns:fx="http://javafx.com/fxml" >
    <stylesheets>
        <URL value="@/css/style.css"/>
    </stylesheets>


    <Button text="Send" onAction="#onClickSendM" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10" prefHeight="30" prefWidth="60"/>
    <TextField fx:id="message" onKeyPressed="#enterPress" AnchorPane.bottomAnchor="10" AnchorPane.rightAnchor="80"
               AnchorPane.leftAnchor="10" prefHeight="30"/>
    <TextArea fx:id="communicationWindow" editable="false" AnchorPane.bottomAnchor="50" AnchorPane.topAnchor="10"
              AnchorPane.rightAnchor="10" AnchorPane.leftAnchor="10" />
</AnchorPane>